@model List<Sandogh.Application.Users.Query.GetUserAddress.UserAddressQuery>
@{
    ViewData["Title"] = "Home Page";
    Layout = "_LayoutInsidePages";
}
<div class="container">
    <div class="row my-5">
        <div class="card">
            <div class="card-header">
                <div class="card-title">لیست آدرس ها</div>
            </div>
            <div class="card-body">
                @foreach (var item in Model)
                {
                    <div class="row">
                        <div class="col-lg-12">
                            <ul>
                                <li>@item.City</li>
                                <li>@item.State</li>
                                <li>@item.PostalAddress</li>
                                <li>@item.ReciverName</li>

                            </ul>
                        </div>
                    </div>
                    <hr />
                }
            </div>
        </div>
    </div>
</div>